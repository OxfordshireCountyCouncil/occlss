<header class="occlss-page-head" role="banner">
    
    <div class="occlss-wrapper occlss-page-head__wraper occlss-page-head__wraper--border {{ selectedCategoryClass }}"><!--START wrapper block-->
      <div class="occlss-layout occlss-layout--align-start"><!--START layout block-->
        <div class="occlss-layout__col occlss-layout__col--sm-12 occlss-layout__col--bleed occlss-page-head__nav">
          {{> molecules-site-nav }}
        </div>
      </div><!--End layout elemnt -->
      <div class="occlss-layout occlss-layout--gutter-l"><!--START layout block-->
        <div class="occlss-layout__col occlss-layout__col--md-7 occlss-layout--order-2-md occlss-page-head__cat-nav-container"><!--START layout elemnt -->
            {{> molecules-category-nav }}
        </div><!--End layout elemnt -->
         <div class="occlss-layout__col occlss-layout__col--md-5 occlss-layout--order-1-md occlss-page-head__search-container"><!--START layout elemnt -->
            {{> organisms-short-search-form }}
        </div><!--End layout elemnt -->
      </div> <!--End layout block -->

    </div> <!--End wrapper block -->

</header>
