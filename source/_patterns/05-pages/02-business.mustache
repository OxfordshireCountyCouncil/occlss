{{> organisms-page-head-with-form}}
<main id="occlss-main-content">
  <div class="occlss-wrapper">
      {{> molecules-breadcrumb(occlss-breadcrumb-no-space: 'true') }}
  </div>
  <section>
    <div class="occlss-wrapper">
      {{> organisms-hero-pane(occlss-hero-title: "Business", occlss-hero-image: "../../images/demo/hero-image-3.jpg", occlss-hero-mod-class: "occlss-hero-pane--lgimage",occlss-hero-heading-mod: "occlss-hero-pane__heading--business") }}     
    </div>
  </section>
  
<section>
    <div class="occlss-wrapper"><!--START wrapper block-->
      <div class="occlss-layout occlss-layout--gutter-s occlss-u-tspace-sm"><!--START layout block-->
        {{> sections-recent-news-section }}
      </div><!--End layout elemnt -->
    </div>
</section>

 <section role="tablist" aria-live="polite">
    <div class="occlss-wrapper"><!--START wrapper block-->
      <div class="occlss-layout occlss-layout--popular-content-grid occlss-layout--gutter-s"><!--START layout block-->
          {{> sections-categories-grid-section }}
          {{> sections-promoted-content-panel-grid-section }}
        </div><!--End layout elemnt -->
    </div>
</section>

</main>

<section class="occlss-section occlss-section--find-my-nearest">
  {{> organisms-search-place-form }}
</section>

{{> organisms-page-footer }}
