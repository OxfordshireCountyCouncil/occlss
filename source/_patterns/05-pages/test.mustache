<div class="occlss-wrapper" style="margin-top: 50px;"><!--START wrapper block-->
    <div class="occlss-layout"><!--START layout block-->
        <div class="occlss-layout__col occlss-layout__col--12"><!--START layout elemnt -->
            <fieldset class="occlss-form__group">
                <legend class="occlss-form__group-title">Sign in</legend>
                <span>or <a href="javascript:void(0);">create an account</a></span>
                <div class="occlss-form__input-group">
                    <label class="occlss-form__label" for="sign-in-username">Username or email address</label>
                    <input class="occlss-form__input" id="sign-in-username" name="username" type="text" autocapitalize="off" autocorrect="off">
                </div>
                <div class="occlss-form__input-group">
                    <label class="occlss-form__label" for="sign-in-password">Password</label>
                    <input class="occlss-form__input" id="sign-in-password" name="password" type="password">
                </div>
                <p>
                    <a title="Show password" href="javascript:void(0);" aria-controls="sign-in-password">
                        Show password
                    </a>
                </p>
                <button type="submit" class="occlss-button occlss-button--primary occlss-form__submit">Sign in</button>
                <p>
                    <a href="javascript:void(0);" title="Forgot username">Forgot username?</a>
                </p>
                <p>
                    <a href="javascript:void(0);" title="Forgot password">Forgot password?</a>
                </p>
            </fieldset>
        </div>
    </div>
</div>