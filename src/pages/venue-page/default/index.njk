          <!-- END :: Left side -->
          <!-- Right side -->
          <div class="occlss-layout__col occlss-layout__col--md-5 occlss-layout__col--offset-1-md">
              
              <!-- Widget container -->
              <div class="occlss-widget-container">
                <h2 class="occlss-u-bspace-sm">Address</h2>
                <p>
                  Deddington Library<br> 
                  The Old Courthouse<br> 
                  Horse Fair<br> 
                  Deddington<br> 
                  OX15 0SH 
