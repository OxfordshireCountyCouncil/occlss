{% for item in  params.panels %}
	<div class="occlss-summary-pane">
		{% if item.title %}<h2 class="occlss-summary-pane__title">{{ item.title }}</h2>{% endif %}
		
		{% if item.listitems %}
		<ul class="occlss-summary-pane__list">
			{% for listitem in item.listitems %}
			{% if listitem.url %}
			<li class="occlss-summary-pane__listitem"><a href="{{ listitem.url }}">{{ listitem.title }}</a></li>
			{% else %}
			<li class="occlss-summary-pane__listitem">{{ listitem.title }}</li>
			{% endif %}
			{% endfor %}
		</ul>
		{% endif %}
		
	</div>
{% endfor %}